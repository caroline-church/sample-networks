{
  "name": "animaltracking-network",
  "version": "0.0.2",
  "description": "Animal Tracking network based on UK DEFRA regulations",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf -t ./node_modules/ink-docstrap/template .",
    "pkgstamp": "node ./scripts/pkgstamp.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-composer/fabric-composer.git"
  },
  "author": "Fabric Composer",
  "license": "Apache-2",
  "keywords": [
    "defra",
    "farmer",
    "compliance",
    "animal",
    "movement"
  ],
  "devDependencies": {
    "eslint": "^3.6.1",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.1",
    "license-check": "^1.1.5"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./scripts/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "animaltracking-model": "latest"
  }
}
